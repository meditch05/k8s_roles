apiVersion: v1
kind: PersistentVolume
metadata:
  name: registry-pv
  labels:
    kubernetes.io/cluster-service: "true"
    volume-task: registry
spec:
  capacity:
    storage: {{ storage }}Gi
  accessModes:
    - ReadWriteMany
  nfs:
    # FIXME: use the right IP
    server: {{ server }}
    path: {{ path }}
